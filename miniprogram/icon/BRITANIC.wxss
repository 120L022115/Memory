@font-face {
    font-family: "BRITANIC";
    src: url("BRITANIC.eot"); /* IE9 */
    src: url("BRITANIC.eot?#iefix") format("embedded-opentype"), /* IE6-IE8 */
    
    url(data:application/x-font-ttf;charset=utf-8;base64,) format("truetype"), /* chrome、firefox、opera、Safari, Android, iOS 4.2+ */
    
    url("BRITANIC.svg#BRITANIC") format("svg"); /* iOS 4.1- */
    font-style: normal;
    font-weight: normal;
}


.britanic{
    font-family: 'BRITANIC';
}